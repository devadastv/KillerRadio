<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <!--<item-->
    <!--android:drawable="@drawable/ic_contact"-->
    <!--android:state_checked="true"-->
    <!--android:state_pressed="true" />-->
    <!--<item-->
    <!--android:drawable="@drawable/incoming"-->
    <!--android:state_pressed="true" />-->
    <item android:state_checked="true" >
        <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
            <item>
                <shape xmlns:android="http://schemas.android.com/apk/res/android"
                    android:shape="rectangle">
                    <!--<corners-->
                    <!--android:radius="5dp" />-->
                    <!--<solid android:color="#d4e9ff" />-->
                    <stroke
                        android:width="2dp"
                        android:color="@color/calltype_outgoing" />
                </shape>
            </item>

            <item>
                <bitmap
                    android:gravity="center"
                    android:src="@drawable/outgoing_small_selected" />
            </item>
        </layer-list>
    </item>
    <item android:state_checked="false" >
        <layer-list xmlns:android="http://schemas.android.com/apk/res/android">
            <item>
                <shape xmlns:android="http://schemas.android.com/apk/res/android"
                    android:shape="rectangle">
                    <!--<corners-->
                    <!--android:radius="5dp" />-->
                    <!--<solid android:color="#cecece" />-->
                    <stroke
                        android:width="2dp"
                        android:color="@color/calltype_unselected" />
                </shape>
            </item>

            <item>
                <bitmap
                    android:gravity="center"
                    android:src="@drawable/outgoing_small_unselected" />
            </item>
        </layer-list>
    </item>
</selector>